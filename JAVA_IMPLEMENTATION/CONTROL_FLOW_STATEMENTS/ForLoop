package basics;

public class ForLoop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        
        for (int i=10;i<15;i++) {
        	System.out.println("welcome to capgemini");
        }
        
        for (int i=6;i>1;i--) {
        	System.out.println("welcome to pune");
        }
	}

}
